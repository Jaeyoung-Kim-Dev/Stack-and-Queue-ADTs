/**
 * 
 */
package utilities;

import java.io.Serializable;

/**
 * The stack data type operations for a stack
 * 
 * @author Jaeyoung Kim
 *
 */
public interface StackADT<E> extends Serializable {
	/**
	 * Pushes an element to the top of the stack
	 * 
	 * @param e Element
	 */
	public void push(E e);

	/**
	 * Remove and return an element
	 * 
	 * @param e Element
	 */
	public void pop(E e);

	/**
	 * Get the first element without removing it
	 * 
	 * @param e Element
	 */
	public void peek(E e);

	/**
	 * Is there any elements?
	 * 
	 * @param e Element
	 */
	public void isEmpty(E e);

	/**
	 * How many elements are there?
	 * 
	 * @param e Element
	 */
	public void size(E e);

	// public void boolean equals( StackADT<E> that)

}
